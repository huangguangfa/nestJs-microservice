{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/nestjs-microservice/src",
  "monorepo": true,
  "root": "apps/nestjs-microservice",
  "compilerOptions": {
    "webpack": true,
    "tsConfigPath": "apps/nestjs-microservice/tsconfig.app.json"
  },
  "projects": {
    "user-center": {
      "type": "application",
      "root": "apps/user-center",
      "entryFile": "main",
      "sourceRoot": "apps/user-center/src",
      "compilerOptions": {
        "tsConfigPath": "apps/user-center/tsconfig.app.json"
      }
    },
    "system-center": {
      "type": "application",
      "root": "apps/system-center",
      "entryFile": "main",
      "sourceRoot": "apps/system-center/src",
      "compilerOptions": {
        "tsConfigPath": "apps/system-center/tsconfig.app.json"
      }
    },
    "common": {
      "type": "library",
      "root": "libs/common",
      "entryFile": "index",
      "sourceRoot": "libs/common/src",
      "compilerOptions": {
        "tsConfigPath": "libs/common/tsconfig.lib.json"
      }
    }
  }
}
